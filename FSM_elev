package FSM

type State int
const (
		IDLE State = iota
		GOING_UP
		GOING_DOWN
		DOORS_OPEN
		OBSTRUCTION
		STOP
		)

type Event int
const(
		FLOOR_REACHED = iota
		EMERGENCY_STOP
		OBSTRUCTION
		EXEC_ORDER
		RECV_ORDER
		HOLD_DOORS
		FAILURE
		ABORT
		)

type channels struct(
		recv_new_order_channel = chan 
		event_channel = chan Event
		timer_channel = chan bool
		floor_channel = chan int
		)
